{
	"name": "pavlovich-app/yii2-social-login",
	"description": "Yii2 Social Login Extension. (Google, Facebook, Linkedin, Twitter, etc).",
	"keywords": ["yii2", "extension", "login", "openid", "oauth", "authentication"],
	"homepage": "https://github.com/Nodge/yii2-eauth",
	"type": "yii2-extension",
	"license": "New BSD License",
	"authors": [
		{
			"name": "Oleksandr Pavlovych",
			"email": "pavlovych.dev@gmail.com",
			"homepage": "https://github.com/pavlovich-app/yii2-social-login"
		}
	],
	"support": {
		"source": "https://github.com/pavlovich-app/yii2-social-login"
	},
	"autoload": {
		"psr-4": {
			"nodge\\eauth\\": "src/"
		}
	},
	"require": {
		"php": ">=7.0",
		"lib-curl": "*",
		"yiisoft/yii2": "*",
		"lusitanian/oauth": "~0.3.0",
		"nodge/lightopenid": "~1.1.0"
	},
	"extra": {
		"bootstrap": "nodge\\eauth\\Bootstrap"
	}
}